generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Link {
  id           Int       @id @default(autoincrement())
  code         String    @unique
  target       String
  total_clicks Int       @default(0)
  last_clicked DateTime?
  created_at   DateTime  @default(now())
  deleted      Boolean   @default(false)
}
